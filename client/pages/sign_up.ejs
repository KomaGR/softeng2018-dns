<!DOCTYPE html>
<html class="no-js" lang="en">
  <head>
      <%- include navbar.ejs %>

  </head>
<body>
  <h1>This is the sign up page</h1>
  <form action="/sign_up" method="POST">
    <div class="form-group">
      <label for="email">Email</label>
      <input type="email" class="form-control" placeholder="Enter your email" name="s_email" required>
    </div>
    <div class="form-group">
      <label for="Username">Username</label>
      <input type="text" class="form-control" placeholder="Enter your username" name="s_username" required>
    </div>
    <div class="form-group">
      <label for="password">Password</label>
      <input type="text" class="form-control" placeholder="Enter your password" name="s_password" required>
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
  </form>
  <%- include footer.ejs %>
</body>
</html>